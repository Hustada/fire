
<div class="container">
  <h2>Sign up</h2>

<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= f.error_notification %>
   
  <div class="well">
    <div class="form-inputs">
      <ul class="sign-up">
        <li><%= f.input :first_name, required: true %></li>
        
        <li><%= f.input :last_name, required: true %></li>
      
        <li><%= f.input :email, required: true, autofocus: true %></li>
       
        <li><%= f.input :password, required: true %></li>
          
        <li><%= f.input :password_confirmation, required: true %></li>
      </ul>
    </div>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Sign up" %>
  </div>
<% end %>

</div>
